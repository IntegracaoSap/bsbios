   </xs:element>
   <xs:element name="Seals" minOccurs="0">
    <xs:annotation>
     <xs:documentation>Lacres Informações sobre os números dos lacres.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
     <xs:sequence>
      <xs:element name="SealNumber" maxOccurs="unbounded">
       <xs:annotation>
        <xs:documentation>Número do Lacre</xs:documentation>
       </xs:annotation>
       <xs:simpleType>
        <xs:restriction base="xs:string">
         <xs:minLength value="1"/>
         <xs:maxLength value="20"/>
        </xs:restriction>
       </xs:simpleType>
      </xs:element>
     </xs:sequence>
    </xs:complexType>
   </xs:element>
   <xs:element name="Waybills">
    <xs:annotation>
     <xs:documentation>Documentos Fiscais do Vagão</xs:documentation>
    </xs:annotation>
    <xs:complexType>
     <xs:sequence>
      <xs:element name="WaybillInvoiceReference" maxOccurs="unbounded">
       <xs:annotation>
        <xs:documentation>Documento Fiscal</xs:documentation>
       </xs:annotation>
       <xs:complexType>
        <xs:sequence>
         <xs:element name="LoadWeight" type="WeightType" minOccurs="0">
          <xs:annotation>
           <xs:documentation>Peso Rateado Peso deste documento fiscal que foi
                             carregado no Vagão Essa informação será usada para
                             quando uma nota fiscal for usada em mais de um
                             veículo, determinará o percentual da nota fiscal
                             utilizada em cada carga (rateio).</xs:documentation>
          </xs:annotation>
         </xs:element>
        </xs:sequence>
        <xs:attribute name="WaybillInvoceID" type="IDType" use="required">
         <xs:annotation>
          <xs:documentation>ID de Referência do Documento Fiscal</xs:documentation>
         </xs:annotation>
        </xs:attribute>
       </xs:complexType>
      </xs:element>
     </xs:sequence>
    </xs:complexType>
   </xs:element>
   <xs:element name="Items" minOccurs="0">
    <xs:annotation>
     <xs:documentation>Itens (Contêineres) do Vagão</xs:documentation>
    </xs:annotation>
    <xs:complexType>
     <xs:sequence>
      <xs:element name="ItemReference" maxOccurs="unbounded">
       <xs:annotation>
        <xs:documentation>Item do Vagão</xs:documentation>
       </xs:annotation>
       <xs:complexType>
        <xs:attribute name="ItemID" type="IDType" use="required">
         <xs:annotation>
          <xs:documentation>ID de Referência do Item</xs:documentation>
         </xs:annotation>
        </xs:attribute>
       </xs:complexType>
      </xs:element>
     </xs:sequence>
    </xs:complexType>
   </xs:element>
   <xs:element name="ShippingNoteManifests" minOccurs="0">
    <xs:annotation>
     <xs:documentation>Documentos de Transporte Anterior do Vagão</xs:documentation>
    </xs:annotation>
    <xs:complexType>
     <xs:sequence>
      <xs:element name="ShippingNoteManifestReference" maxOccurs="unbounded">
       <xs:annotation>
        <xs:documentation>Documento de Transporte</xs:documentation>
       </xs:annotation>
       <xs:complexType>
        <xs:attribute name="ShippingNoteManifestID" type="IDType"
                      use="required">
         <xs:annotation>
          <xs:documentation>ID de Referência do Documento de Transporte</xs:documentation>
         </xs:annotation>
        </xs:attribute>
       </xs:complexType>
      </xs:element>
     </xs:sequence>
    </xs:complexType>
   </xs:element>
   <xs:element name="Loading" minOccurs="0">
    <xs:annotation>
     <xs:documentation>Vale: Será preenchido somente pela Vale.</xs:documentation>
    </xs:annotation>
    <xs:complexType>
     <xs:sequence>
      <xs:element name="LoadingSerial">
       <xs:annotation>
        <xs:documentation>Série do Carregamento registrado pela Vale.</xs:documentation>
       </xs:annotation>
      </xs:element>
      <xs:element name="LoadingNumber">
       <xs:annotation>
        <xs:documentation>Número do Carregamento registrado pela Vale.</xs:documentation>
       </xs:annotation>
      </xs:element>
     </xs:sequence>
    </xs:complexType>
   </xs:element>
   <xs:element name="ClientReferenceId" minOccurs="0">
    <xs:annotation>
     <xs:documentation>Código do Documento Fiscal no Cliente Sera o documento de
                       referencia utilizado pelo cliente para a emissao do
                       documento fiscal da mercadoria. Os documentos podem ser
                       por exemplo: Ordem de Venda, Ordem, Encomenda, Pedido,
                       etc.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
     <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="20"/>
     </xs:restriction>
    </xs:simpleType>
   </xs:element>
  </xs:sequence>
 </xs:complexType>
 <xs:complexType name="CommodityIdentificationType">
  <xs:annotation>
   <xs:documentation>Tipo de dado para definição de um código de mercadoria
                     dentro de alguma codificação padrão combinada entre as
                     partes envolvidas</xs:documentation>
  </xs:annotation>
  <xs:sequence>
   <xs:element name="CommodityIdentifierDomain">
    <xs:annotation>
     <xs:documentation>Domínio do Código da Mercadoria Este campo indica que
                       padrão/domínio/tipo de codificação o parceiro está
                       enviando. Valores Possíveis: VALE - Codificação interna
                       da Vale MRS - Codificação interna da MRS ALL -
                       Codificação interna da ALL NCM - Codificação do Mercosul
                       (Nomenclatura Comum Mercosul) Outros domínios podem ser
                       acrescidos desde que previamente acordados</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
     <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="10"/>
      <xs:enumeration value="VALE"/>
      <xs:enumeration value="MRS"/>
      <xs:enumeration value="ALL"/>
      <xs:enumeration value="NCM"/>
     </xs:restriction>
    </xs:simpleType>
   </xs:element>
   <xs:element name="CommodityIdentifier">
    <xs:annotation>
     <xs:documentation>Código da Mercadoria propriamente dito segundo padrão
                       selecionado no campo 'Domain'.</xs:documentation>
    </xs:annotation>
    <xs:simpleType>
     <xs:restriction base="xs:string">
      <xs:minLength value="1"/>
      <xs:maxLength value="10"/>
     </xs:restriction>
    </xs:simpleType>
   </xs:element>
  </xs:sequence>
 </xs:complexType>
